[sub_resource type="GDScript" id="GDScript_h60rk"]
script/source = "extends Node3D

@export var id_1: int
@export var id_2: int
@onready var spawn_1: Node3D = $Spawn1
@onready var spawn_2: Node3D = $Spawn2
var hit_1:=false
var hit_2:=false
var chum1
var chum2

func _ready() -> void:
	chum1 = ChumsManager.get_specific_chum_id(id_1).instantiate()
	add_child(chum1)
	chum1.global_position = spawn_1.global_position
	chum1.health_node.set_max_health_override(500)
	chum1.make_enemy()
	chum1.set_state(\"Wake\")
	chum1.hurtbox.recieved_damage.connect(damaged_1)
	
	chum2 = ChumsManager.get_specific_chum_id(id_2).instantiate()
	add_child(chum2)
	chum2.global_position = spawn_2.global_position
	chum2.health_node.set_max_health_override(500)
	chum2.make_friendly()
	chum2.set_state(\"Wake\")
	chum2.hurtbox.recieved_damage.connect(damaged_2)

func damaged_1(_a, _b, _c):
	hit_1 = true
func damaged_2(_a, _b, _c):
	hit_2 = true

func _on_timer_timeout() -> void:
	if not hit_1:
		print(\"%s did not hit %s!\" % [chum2.chum_name, chum1.chum_name])
	if not hit_2:
		print(\"%s did not hit %s!\" % [chum1.chum_name, chum2.chum_name])
"

