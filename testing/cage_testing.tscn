[sub_resource type="GDScript" id="GDScript_h60rk"]
script/source = "extends Node3D

@export var id_1: int
@export var id_2: int
@onready var spawn_1: Node3D = $Spawn1
@onready var spawn_2: Node3D = $Spawn2
var hit_1:=false
var hit_2:=false

func _ready() -> void:
	var chum1 = ChumsManager.get_specific_chum_id(id_1).instantiate()
	add_child(chum1)
	chum1.global_position = spawn_1.global_position
	
	chum1.make_enemy()
	chum1.set_state(\"Wake\")
	chum1.hurtbox.recieved_damage.connect(damaged_1)
	
	var chum2 = ChumsManager.get_specific_chum_id(id_2).instantiate()
	add_child(chum2)
	chum2.global_position = spawn_2.global_position
	
	chum2.make_friendly()
	chum2.set_state(\"Wake\")
	chum2.hurtbox.recieved_damage.connect(damaged_2)

func damaged_1(a, b, c):
	hit_1 = true
func damaged_2(a, b, c):
	hit_2 = true

func _on_timer_timeout() -> void:
	if not hit_1 and hit_2:
		print(\"From %s, %s. Only %s has been hit\" % [id_1, id_2, id_2])
	if not hit_2 and hit_1:
		print(\"From %s, %s. Only %s has been hit\" % [id_1, id_2, id_1])
	if not hit_1 and not hit_2:
		print(\"From %s, %s. Neither have been hit\")
	
"

